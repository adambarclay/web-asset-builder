<Project>
	<Target Name="CreateMissingManifest" BeforeTargets="BeforeCompile">
		<WebAssetBuilderMissingManifestTask AssemblyName="$(AssemblyName)"/>
	</Target>
	<Target Name="BuildWebAssets" BeforeTargets="Build;Rebuild">
		<WebAssetBuilderTask AssemblyName="$(AssemblyName)" AssetOutputPath="$(AssetOutputPath)" FileTypesToCompress="$(FileTypesToCompress)" ProjectFiles="@(None)"/>
	</Target>
</Project>
